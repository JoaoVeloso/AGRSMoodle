;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cluster.agrsmoodle.pt. admin.agrsmoodle.pt. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	cluster.agrsmoodle.pt.
2	IN	PTR	cluster.agrsmoodle.pt.
50	IN	PTR	moodleserver1.agrsmoodle.pt.
99	IN	PTR	moodleserver2.agrsmoodle.pt.
